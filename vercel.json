{
  "rewrites": [
    {
      "source": "/old-route",
      "destination": "/new-route"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    }
  ],
  "redirects": [
    {
      "source": "/legacy-page",
      "destination": "/new-page",
      "statusCode": 301
    }
  ],
  "headers": [
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}